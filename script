let list_site = ['web.whatsapp.com/', 'mail.ru/'];

let i=0; while(i!=list_site.length){
	if (window.location.toString().indexOf(list_site[i]) > 0) {
	
      	$('body').children().remove();
    		$('body').append('<h1 style="background-color: red">Доступ к сайту запрещен!</h1>');
     

	}

i++;}



	

